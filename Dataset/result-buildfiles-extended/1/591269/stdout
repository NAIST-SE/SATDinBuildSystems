Tests\" \\\n\t\t\"$(DESTDIR)$(portage_base)/pym/portage/tests\"; \\\n\tcd \"$(srcdir)/pym/portage/tests\"; \\\n\tinstall -m$(INSMODE) *.py \\\n\t\t\"$(DESTDIR)$(portage_base)/pym/portage/tests\"; \\\n\tinstall -d -m$(DIRMODE) \\\n\t\t\"$(DESTDIR)$(portage_base)/pym/portage/tests/lint\"; \\\n\tcd \"$(srcdir)/pym/portage/tests/lint\"; \\\n\tinstall -m$(INSMODE) *.py __test__ \\\n\t\t\"$(DESTDIR)$(portage_base)/pym/portage/tests/lint\"; \\\n\t\\\n\tinstall -d -m$(DIRMODE) \"$(DESTDIR)$(docdir)\"; \\\n\tcd \"$(srcdir)\"; \\\n\tinstall -m $(INSMODE) $(DOCS) \"$(DESTDIR)$(docdir)\"; \\\n\t\\\n\tfor x in \"\" $(LINGUAS); do \\\n\t\tfor y in 1 5 ; do \\\n\t\t\tif [ -d \"$(srcdir)/man/$$x\" ]; then \\\n\t\t\t\tcd \"$(srcdir)/man/$$x\"; \\\n\t\t\t\tfiles=$$(echo *.$$y); \\\n\t\t\t\tif [ -z \"$$files\" ] || [ \"$$files\" = \"*.$$y\" ]; then \\\n\t\t\t\t\tcontinue; \\\n\t\t\t\tfi; \\\n\t\t\t\tinstall -d -m$(DIRMODE) \"$(DESTDIR)$(mandir)/$$x/man$$y\"; \\\n\t\t\t\tinstall -m$(INSMODE) *.$$y \"$(DESTDIR)$(mandir)/$$x/man$$y\"; \\\n\t\t\tfi; \\\n\t\tdone; \\\n\tdone; \\\n\t\\\n\tif [ -f \"$(srcdir)/doc/portage.html\" ] ; then \\\n\t\tinstall -d -m$(DIRMODE) \"$(DESTDIR)$(htmldir)\"; \\\n\t\tcd \"$(srcdir)/doc\"; \\\n\t\tinstall -m$(INSMODE) *.html \"$(DESTDIR)$(htmldir)\"; \\\n\tfi; \\\n\t\\\n\tif [ -d \"$(WORKDIR)/epydoc\" ] ; then \\\n\t\tinstall -d -m$(DIRMODE) \"$(DESTDIR)$(htmldir)\"; \\\n\t\tcp -pPR \"$(WORKDIR)/epydoc\" \\\n\t\t\t\"$(DESTDIR)$(htmldir)/api\"; \\\n\t\tcd \"$(DESTDIR)$(htmldir)/api\"; \\\n\t\tfind . -type d | xargs chmod $(DIRMODE); \\\n\t\tfind . -type f | xargs chmod $(INSMODE); \\\n\tfi; \\\n\n",
